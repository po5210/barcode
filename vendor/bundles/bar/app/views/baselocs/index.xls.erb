<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
		<Cell><Data ss:Type="String">Baseloc Cd</Data></Cell>
		<Cell><Data ss:Type="String">Baseloc Nm</Data></Cell>
		<Cell><Data ss:Type="String">Baseloc Flag</Data></Cell>
		<Cell><Data ss:Type="String">Div Cd</Data></Cell>
		<Cell><Data ss:Type="String">Inloc Cd</Data></Cell>		
		<Cell><Data ss:Type="String">Outloc Cd</Data></Cell>
		<Cell><Data ss:Type="String">Baseloc Nmk</Data></Cell>
		<Cell><Data ss:Type="String">Use Y/N</Data></Cell>
      </Row>
    <% @collection.each do |item| %>
      <Row>
		<Cell><Data ss:Type="String"><%= item["baseloc_cd"] %></Data></Cell>
		<Cell><Data ss:Type="String"><%= item["baseloc_nm"] %></Data></Cell>
		<Cell><Data ss:Type="String"><%= item["baseloc_fg"] %></Data></Cell>
		<Cell><Data ss:Type="String"><%= item["div_cd"] %></Data></Cell>
		<Cell><Data ss:Type="String"><%= item["inloc_cd"] %></Data></Cell>
		<Cell><Data ss:Type="String"><%= item["outloc_cd"] %></Data></Cell>
		<Cell><Data ss:Type="String"><%= item["baseloc_nmk"] %></Data></Cell>		
		<Cell><Data ss:Type="String"><%= item["use_yn"] %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>